#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/d/前端项目/新建文件夹 (3)/blog-server/node_modules/.pnpm/tldts@6.1.86/node_modules/tldts/bin/node_modules:/mnt/d/前端项目/新建文件夹 (3)/blog-server/node_modules/.pnpm/tldts@6.1.86/node_modules/tldts/node_modules:/mnt/d/前端项目/新建文件夹 (3)/blog-server/node_modules/.pnpm/tldts@6.1.86/node_modules:/mnt/d/前端项目/新建文件夹 (3)/blog-server/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/d/前端项目/新建文件夹 (3)/blog-server/node_modules/.pnpm/tldts@6.1.86/node_modules/tldts/bin/node_modules:/mnt/d/前端项目/新建文件夹 (3)/blog-server/node_modules/.pnpm/tldts@6.1.86/node_modules/tldts/node_modules:/mnt/d/前端项目/新建文件夹 (3)/blog-server/node_modules/.pnpm/tldts@6.1.86/node_modules:/mnt/d/前端项目/新建文件夹 (3)/blog-server/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../tldts@6.1.86/node_modules/tldts/bin/cli.js" "$@"
else
  exec node  "$basedir/../../../../../tldts@6.1.86/node_modules/tldts/bin/cli.js" "$@"
fi
